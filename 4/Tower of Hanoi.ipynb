    "def TOH(n, src, aux, dest):\n",
    "    if n == 1:\n",
    "        print(\"I moved from \"+src+\" to \" + dest)\n",
    "        return\n",
    "    TOH( n-1, src, dest, aux)\n",
    "    print(\"I moved from \"+src+\" to \" + dest)\n",
    "    TOH( n-1, aux, src, dest)\n",
    "n = 3\n",
    "TOH(n, 'src', 'aux', 'dest')"
   ]
